{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "custom-block/contact-details",
	"version": "0.1.0",
	"title": "Deappro Custom Block",
	"category": "design",
	"icon": "hammer",
	"attributes": {
		"companyName": {
			"type": "string",
			"source": "html",
			"selector": ".company-name"
		},
		"CompanyEmail": {
			"type": "string",
			"source": "html",
			"selector": ".company-email"
		}
	},
	"description": "This is my awesome contact form",
	"supports": {
		"html": false
	},
	"textdomain": "deappro-custom-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
